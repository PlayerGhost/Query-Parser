const teste = "Select idusuario, nome, datanascimento, descricao, saldoinicial, UF, Descrição from usuario join contas on usuario.idUsuario = contas.Usuario_idUsuario join tipoconta on tipoconta.idTipoConta = contas.TipoConta_idTipoConta where saldoinicial < 3000 and uf = 'ce' and Descrição <> 'Conta Corrente' and idusuario not in (1,2,3)"

arguments = teste.split(' ')

let bodies = []

const result = () => {
    let aux=""

    for([index, word] of arguments.entries()){
        
        switch(word){
            case "Select":
                arguments[index] = "π"
                break

            case "where":
                bodies.push(aux)
                aux=""
                arguments[index] = "σ"
                break

            case "from":
                bodies.push(aux)
                aux=""
                break
            case "join":
                bodies.push(aux)
                aux=""
                break

        }
        aux+=word+" "
        
    }
    bodies.push(aux)
    return aux
}
result()
console.log(bodies)
